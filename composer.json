{
    "name": "biurad/nette-di-bridge",
    "type": "library",
    "description": "The DependencyInjection allows you to standardize and centralize the way objects are constructed in your application.",
    "keywords": [
        "container",
        "dependency-injection",
        "nette",
        "nette-di",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": ">=7.1",
        "nette/di": "^3.0",
        "biurad/biurad-helpers": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\DependencyInjection\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\DependencyInjection\\Tests\\": "Tests/"
        }
    },
    "suggest": {
        "biurad/biurad-loader": "Required to load service definitions from xml and ini files",
        "biurad/doctrine-annotations-bridge": "Required to use container with annotations"
    },
    "minimum-stability": "dev"
}
